<h1>These are my recipes:</h1>
<% @recipes.each do |recipe| %>
  <ul>
    <a href="/recipes/<%= recipe.id %>">
      <%= recipe.name %>
    </a>
    <a href="/recipes/<%= recipe.id %>">
      <%= recipe.ingredients %>
    </a>
    <a href="/recipes/<%= recipe.id %>">
      <%= recipe.cook_time %>
    </a>
  </ul>
<% end %>
